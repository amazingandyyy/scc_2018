{
  "name": "scc-2018-client",
  "version": "1.0.1",
  "description": "Simple starter package for Redux with React and Babel support",
  "main": "index.js",
  "scripts": {
    "start": "serve ../docs",
    "dev": "webpack-dev-server --open --config webpack.dev.js",
    "build": "rm -rf ../docs && webpack -p --config webpack.prod.js --optimize-minimize && npm run copy-files",
    "copy-files": "npm run copy-manifest && npm run copy-readme",
    "copy-manifest": "cp src/manifest.json ./docs",
    "copy-readme": "cp ./readme.md ./docs"
  },
  "author": "amazingandyyy",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "copy-webpack-plugin": "^4.3.1",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "image-webpack-loader": "^4.0.0",
    "json-loader": "^0.5.7",
    "no-console-webpack-plugin": "0.0.5",
    "node-sass": "^4.5.3",
    "sass-loader": "^6.0.6",
    "serve": "^6.1.0",
    "style-loader": "^0.20.1",
    "uglifyjs-webpack-plugin": "^1.1.8",
    "webpack": "^3.6.0",
    "webpack-bundle-analyzer": "^2.10.0",
    "webpack-dev-server": "^2.8.2",
    "webpack-merge": "^4.1.0",
    "eslint": "^4.14.0",
    "eslint-config-logux": "^17.0.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-es5": "^1.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.5.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.5.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-standard": "^3.0.1"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "bootstrap": "^4.0.0-beta.2",
    "jquery": "^3.3.1",
    "lodash": "^4.17.4",
    "preact": "^8.2.7",
    "preact-compat": "^3.18.0",
    "prop-types": "^15.6.0",
    "qrcode": "^1.2.0",
    "qs": "^6.5.1",
    "querystring": "^0.2.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-dropzone": "^4.2.8",
    "react-image": "^1.3.1",
    "react-recaptcha": "^2.3.7",
    "react-redux": "^5.0.6",
    "react-redux-loading-bar": "^3.1.1",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-form": "^7.2.3",
    "redux-promise-middleware": "^5.0.0",
